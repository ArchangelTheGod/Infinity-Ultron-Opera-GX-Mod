{
    "name": "Infinity Ultron (Marvel Studios: What If?)",
    "description": "An Infinity Ultron mod revolving around the unique details of Infinity Ultron",
    "developer":
    {
        "name": "ArchangelTheGod"
    },
    "icons":
    {
        "512": "icon_512.jpg"
    },
    "manifest_version": 3,
    "mod":
    {
        "license": "license.txt",
        "payload":
        {
            "browser_sounds":
            {
                "CLICK":
                [
                    "sound/click.mp3"
                ],
                "FEATURE_SWITCH_OFF":
                [
                    "sound/f_off.mp3"
                ],
                "FEATURE_SWITCH_ON":
                [
                    "sound/f_on.mp3"
                ],
                "HOVER":
                [
                    "sound/hover.mp3"
                ],
                "HOVER_UP":
                [
                    "sound/hover.mp3"
                ],
                "IMPORTANT_CLICK":
                [
                    "sound/impclick.mp3"
                ],
                "LEVEL_UPGRADE":
                [
                    "sound/open.mp3"
                ],
                "LIMITER_OFF":
                [
                    "sound/f_off.mp3"
                ],
                "LIMITER_ON":
                [
                    "sound/open.mp3"
                ],
                "SWITCH_TOGGLE":
                [
                    "sound/f_off.mp3"
                ],
                "TAB_CLOSE":
                [
                    "sound/close.mp3"
                ],
                "TAB_INSERT":
                [
                    "sound/open.mp3"
                ],
                "TAB_SLASH":
                [
                    "sound/close.mp3"
                ]
            },

            "keyboard_sounds":
            {
                "TYPING_BACKSPACE":
                [
                    "keyboard/Backspace.mp3"
                ],
                "TYPING_ENTER":
                [
                    "keyboard/Enter1.mp3"
                ],
                "TYPING_SPACE":
                [
                    "keyboard/Space.mp3"
                ]
            },

            "page_styles":
            [
                {
                    "css":
                    [
                        "webmodding/opera.css"
                    ],
                    "matches":
                    [
                        "https://*.opera.com/*"
                    ]
                }
            ],
            "theme":
            {
                "dark":
                {
                    "gx_accent":
                    {
                        "h": 271,
                        "s": 50,
                        "l": 46
                    },
                    "gx_secondary_base":
                    {
                        "h": 360,
                        "s": 77,
                        "l": 16
                    }
                },
                "light":
                {
                    "gx_accent":
                    {
                        "h": 16,
                        "s": 56,
                        "l": 53
                    },
                    "gx_secondary_base":
                    {
                        "h": 360,
                        "s": 100,
                        "l": 16
                    }
                }
            },
            "wallpaper":
            {
                "dark":
                {   
                    "image": "wallpaper/walp1.png",
                    "text_color": "#FFFFFF",
                    "text_shadow": "#757575"
                },
                "light":
                {
                    "image": "wallpaper/walp2.jpeg",
                    "text_color": "#FFFFFF",
                    "text_shadow": "#0B000E"
                }
            }
        },
        "schema_version": 1
    },
    "version": "1.0"
}
